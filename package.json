{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --coverage --silent --ci",
    "generate:nexus": "nodemon --exec 'ts-node --transpile-only -P nexus.tsconfig.json pages/api/graphql' --ext 'ts' --watch '*/graphql/**/*.ts'",
    "generate:genql": "nodemon --exec 'genql --schema ./graphql/schema.graphql --output ./graphql/generated/genql'  --watch 'graphql/schema.graphql'",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  },
  "prisma": {
    "seed": "ts-node -O {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "@prisma/client": "^3.9.2",
    "axios": "^0.26.0",
    "clsx": "^1.1.1",
    "daisyui": "^2.1.0",
    "next": "latest",
    "next-auth": "^4.2.1",
    "nextjs-progressbar": "^0.0.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.27.1",
    "swiper": "^8.0.6",
    "swr": "^1.2.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@testing-library/dom": "^8.11.3",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^14.0.0-beta",
    "@types/node": "17.0.4",
    "@types/react": "17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-testing-library": "^5.0.5",
    "husky": ">=6",
    "jest": "^27.5.1",
    "lint-staged": ">=10",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "prisma": "^3.9.2",
    "tailwindcss": "^3.0.7",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "4.5.4"
  },
  "lint-staged": {
    "*.(tsx|ts|js)": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write --ignore-unknown"
  }
}
